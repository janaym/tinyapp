<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href = "/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <button class="btn btn-outline-light" onclick="window.location ='http://localhost:8080/register';">Register</button>
    <% if (!user) { %>
      <div class="navbar-dark" >
        <button class ="btn btn-outline-light" onclick="window.location ='http://localhost:8080/login';">Login</button>
      </div>
    <% } else { %>
      <form class = "form-inline" method="post" action="/logout">
        <button type="submit" class="btn btn-outline-light" name="logout">Logout</button>
        <label for="logout" class="text-light">Logged in as: <%= user.email %> </label>
      </form>
    <% } %>
   
  </nav>
</header>